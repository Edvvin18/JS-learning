class pessoa {
    constructor(nome) {
        this.nome = nome
    }

    falar() {
        console.log(`Meu nome eh ${this.nome}`)
    }
}
const p1 = new pessoa("Edwin")
p1.falar()

const criarPessoa = nome => {
    return {
        falar: () => console.log(`Meu nome eh ${nome}`)

    }
}

const p2 = criarPessoa("Junior")
p2.falar()